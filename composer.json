{
    "name": "realhood/assemblyai-realtime",
    "description": "PHP package for AssemblyAI real-time streaming with Lemur support",
    "type": "library",
    "keywords": [
        "assemblyai",
        "transcription",
        "speech-to-text",
        "lemur",
        "ai",
        "websocket",
        "real-time"
    ],
    "homepage": "https://github.com/realhood/assemblyai-realtime-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Yasin",
            "email": "aleyasin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ratchet/pawl": "^0.4.1",
        "guzzlehttp/guzzle": "^7.0",
        "react/event-loop": "^1.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AssemblyAIRealtime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests",
        "analyse": "phpstan analyse src tests --level=7",
        "all": [
            "@check-style",
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}